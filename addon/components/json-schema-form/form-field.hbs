{{#if this.formField}}
  {{#let this.formField as |formField|}}
    <formField
      @data={{@data}}
      @dataSchema={{@dataSchema}}
      @formId={{@formId}}
      @elementSchema={{@elementSchema}}
      @onValueChange={{@onValueChange}}
      @onValueInitialized={{@onValueInitialized}}
      ...attributes/>
  {{/let}}
{{/if}}